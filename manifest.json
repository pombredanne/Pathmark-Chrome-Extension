{
  "name": "Spider",
  "version": "1.1",
  "description": "Reads your history, and shows the top ten pages you go to by typing the URL.",
  "permissions": ["history", "tabs", "webNavigation"],
  "browser_action": {
    "default_popup": "spider.html",
    "default_icon": "clock.png"
  },
  "background": {
    "scripts": ["daemon.js"],
    "persistent": true
  },
  "manifest_version": 2
}
